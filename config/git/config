[alias]
	a = add
	b = branch
	c = checkout
	d = diff
	f = fetch
	l = log
	m = merge
	p = pull
	pu = push
	r = rebase
	re = reset
	rev = revert
	s = status
	sh = show
	st = stash
	sw = switch
[commit]
	gpgsign = true
[core]
	pager = delta
	quotepath = false
[delta]
	side-by-side = true
	syntax-theme = GitHub
	tabs = 2
[diff]
	algorithm = histogram
	colorMoved = default
	external = difft
	tool = difftastic
[difftool]
	prompt = false
[difftool "difftastic"]
	cmd = difft "$LOCAL" "$REMOTE"
[fetch]
	prune = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	process = git-lfs filter-process
	required = true
	smudge = git-lfs smudge -- %f
[gpg]
	format = ssh
[gpg "ssh"]
	program = /Applications/1Password.app/Contents/MacOS/op-ssh-sign
[init]
	defaultBranch = main
[log]
	abbrevCommit = true
	date = iso8601
[merge]
	conflictStyle = zdiff3
[pager]
	difftool = true
[pull]
	rebase = true
[push]
	autoSetupRemote = true
	default = current

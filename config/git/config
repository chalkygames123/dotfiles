[commit]
	gpgsign = true
[core]
	pager = delta
	quotepath = false
[delta]
	interactive = true
	side-by-side = true
	syntax-theme = GitHub
	tabs = 2
[diff]
	external = difft
[fetch]
	all = true
	prune = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	process = git-lfs filter-process
	required = true
	smudge = git-lfs smudge -- %f
[format]
	pretty = oneline
[gpg]
	format = ssh
[gpg "ssh"]
	program = /Applications/1Password.app/Contents/MacOS/op-ssh-sign
[init]
	defaultBranch = main
[log]
	abbrevCommit = true
	date = iso-local
[pull]
	rebase = true
[push]
	autoSetupRemote = true
	default = current
	useForceIfIncludes = true
[rerere]
	enabled = true

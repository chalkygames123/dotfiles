 defaults write com.apple.dock ResetLaunchPad -bool true; killall Dock
 fd -H '^\.DS_Store$' -tf -X rm
 rm ~/Library/Application\ Support/Google/Chrome/Default/Favicons
 tree -aF -I '.git|dist|node_modules'
 vi ~/.gitconfig
 vi ~/.zshrc
 vi ~/.zsh_history
 brew upgrade; brew cleanup
 dg commit -m "Update $(dg diff --name-only --staged | sed -e 's/$/, /' | tr -d '\n' | sed -e 's/\(.*\), /\1/' | sed -e 's/\(.*\), /\1 and /')"
 gg status --ignored --show-stash
 git reflog expire --expire=now --all
 git clean -fdx -e node_modules
 git clean -fdxn -e node_modules
 git diff --staged
 git commit --amend --no-edit
 git commit -m ''
 exec $SHELL -l
